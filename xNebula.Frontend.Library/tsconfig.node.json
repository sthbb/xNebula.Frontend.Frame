{
  "include": ["vite.config.*", "scripts/**/*.ts", "examples/*/*.ts"],
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "lib": ["ES2023"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["node"],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo"
  }
}
